      "source": [
        "# Install required libraries\n",
        "!pip install --upgrade pip\n",
        "!pip install lxml_html_clean\n",
        "!pip install nltk spacy newspaper3k transformers\n",
        "!python -m spacy download en_core_web_sm\n",
        "\n",
        "import nltk\n",
        "nltk.download('punkt')\n",
        "nltk.download('stopwords')\n",
        "nltk.download('averaged_perceptron_tagger')\n",
        "nltk.download('wordnet')\n",
        "\n",
        "from newspaper import Article\n",
        "import re\n",
        "from nltk.tokenize import word_tokenize, sent_tokenize\n",
        "from nltk.corpus import stopwords\n",
        "from nltk.stem import WordNetLemmatizer\n",
        "from nltk import pos_tag\n",
        "import spacy\n",
        "from collections import Counter\n",
        "from transformers import pipeline\n",
        "\n",
        "# Initialize NLP components\n",
        "nlp = spacy.load(\"en_core_web_sm\")\n",
        "lemmatizer = WordNetLemmatizer()\n",
        "stop_words = set(stopwords.words('english'))\n",
        "\n",
        "def get_article_content():\n",
        "    \"\"\"Try multiple news sources with fallback to sample text\"\"\"\n",
        "    urls = [\n",
        "        \"https://techcrunch.com/2024/01/15/openai-reveals-plan-for-ai-safety/\",\n",
        "        \"https://www.reuters.com/technology/ai-risks-need-global-action-openai-microsoft-others-say-2024-01-15/\",\n",
        "        \"https://www.theverge.com/2024/1/15/24038960/openai-ai-safety-preparedness-team\"\n",
        "    ]\n",
        "\n",
        "    for url in urls:\n",
        "        try:\n",
        "            article = Article(url)\n",
        "            article.download()\n",
        "            article.parse()\n",
        "            return article.text\n",
        "        except:\n",
        "            continue\n",
        "\n",
        "    # Fallback sample text if all URLs fail\n",
        "    return \"\"\"\n",
        "    Artificial intelligence is advancing rapidly, with new models being developed every month.\n",
        "    Companies like OpenAI, Google, and Microsoft are leading the AI revolution.\n",
        "    Recent developments in natural language processing have enabled computers to understand\n",
        "    and generate human-like text. However, experts warn about potential risks including\n",
        "    job displacement and misinformation. Governments worldwide are now working on\n",
        "    regulations to ensure safe and ethical AI development. The future of AI looks\n",
        "    promising but requires careful oversight to benefit humanity.\n",
        "    \"\"\"\n",
        "\n",
        "def clean_text(text):\n",
        "    \"\"\"Clean and preprocess text\"\"\"\n",
        "    text = re.sub(r'[^a-zA-Z\\s]', '', text)\n",
        "    text = text.lower()\n",
        "    text = ' '.join(text.split())\n",
        "    return text\n",
        "\n",
        "def process_text(text):\n",
        "    \"\"\"Tokenize, lemmatize and remove stopwords\"\"\"\n",
        "    tokens = word_tokenize(text)\n",
        "    return [lemmatizer.lemmatize(token) for token in tokens if token not in stop_words]\n",
        "\n",
        "def analyze_content(text):\n",
        "    \"\"\"Perform all NLP analyses\"\"\"\n",
        "    print(\"=== Original Text Sample ===\")\n",
        "    print(text[:300] + \"...\\n\")\n",
        "\n",
        "    cleaned = clean_text(text)\n",
        "    print(\"=== Cleaned Text Sample ===\")\n",
        "    print(cleaned[:300] + \"...\\n\")\n",
        "\n",
        "    tokens = process_text(cleaned)\n",
        "    print(\"=== Tokens Sample (20) ===\")\n",
        "    print(tokens[:20], \"\\n\")\n",
        "\n",
        "    pos_tags = pos_tag(tokens)\n",
        "    print(\"=== POS Tags Sample (20) ===\")\n",
        "    print(pos_tags[:20], \"\\n\")\n",
        "\n",
        "    doc = nlp(text)\n",
        "    noun_phrases = [chunk.text for chunk in doc.noun_chunks]\n",
        "    key_phrases = Counter(noun_phrases).most_common(5)\n",
        "    print(\"=== Top 5 Key Phrases ===\")\n",
        "    print([phrase for phrase, count in key_phrases], \"\\n\")\n",
        "\n",
        "    # Basic summary using sentence scoring\n",
        "    sentences = sent_tokenize(text)\n",
        "    word_freq = Counter([word.lower() for word in word_tokenize(text) if word.lower() not in stop_words])\n",
        "    ranked_sentences = sorted(sentences, key=lambda s: sum(word_freq[word.lower()] for word in word_tokenize(s)), reverse=True)\n",
        "    basic_summary = ' '.join(ranked_sentences[:3])\n",
        "\n",
        "    # Transformer-based summary\n",
        "    try:\n",
        "        summarizer = pipeline(\"summarization\", model=\"facebook/bart-large-cnn\")\n",
        "        transformer_summary = summarizer(text, max_length=150, min_length=50, do_sample=False)[0]['summary_text']\n",
        "    except:\n",
        "        transformer_summary = \"Transformer summary unavailable\"\n",
        "\n",
        "    print(\"=== Basic Summary ===\")\n",
        "    print(basic_summary, \"\\n\")\n",
        "\n",
        "    print(\"=== Transformer Summary ===\")\n",
        "    print(transformer_summary)\n",
        "\n",
        "    return {\n",
        "        'cleaned_text': cleaned,\n",
        "        'tokens': tokens,\n",
        "        'pos_tags': pos_tags,\n",
        "        'key_phrases': key_phrases,\n",
        "        'basic_summary': basic_summary,\n",
        "        'transformer_summary': transformer_summary\n",
        "    }\n",
        "\n",
        "# Main execution\n",
        "print(\"Starting NLP processing...\\n\")\n",
        "article_text = get_article_content()\n",
        "results = analyze_content(article_text)\n",
        "print(\"\\nProcessing complete!\")"
      ]
    },
